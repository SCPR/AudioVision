<div class="billboard layout-5 bound">

    <div class="row primary clearfix">
        <%= render "billboards/components/mobile", post: billboard.posts[0] %>
        <%= render "billboards/components/video", post: billboard.posts[1] %>
    </div><!--/ .row-primary -->


    <div class="row secondary clearfix">
    <% Array(billboard.posts[2..4]).each do |post| %>
        <%= render "billboards/components/image", post: post, size: :small %>
    <% end %>
    </div><!--/ .row-secondary -->

</div><!--/ .billboard -->
